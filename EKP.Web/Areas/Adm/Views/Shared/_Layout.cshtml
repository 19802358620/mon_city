<body class="page-sidebar-closed-hide-logo page-sidebar-closed-hide-logo">
    <div class="page-header navbar navbar-static-top" id="page-header">
        <div class="page-header-inner">
            <div class="page-logo">
                <a href="javascript:;" onclick="goUrl(this, 'Home', 'Index')">
                    <img id="img_logo"   class="logo-default">
                </a>
                <div class="menu-toggler sidebar-toggler">
                </div>
            </div>
            <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse"></a>
            <div class="hor-menu hor-menu-light hidden-sm hidden-xs">
                <ul class="nav navbar-nav" id="navTop"></ul>
            </div>
            <div class="page-top">
                <div class="top-menu">
                    <ul class="nav navbar-nav pull-right">
                        <li class="dropdown dropdown-extended dropdown-dark dropdown-inbox dropdown-shared" id="header_shared_bar" style="display: none">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true" data-delay="999999">
                                <i class="icon-home float-left"></i>&nbsp;<span class="badge">更多模板...</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="external">
                                    <h3>当前有 <strong>2 个</strong> 模板可使用</h3>
                                </li>
                                <li>
                                    <div class="mix-grid">
                                        <div class="col-md-6 mix mix_all" d-name="_Layout">
                                            <div class="mix-inner">
                                                <div class="img-box">
                                                    <img class="img-responsive" src="/Areas/Adm/Images/_Layout.png">
                                                </div>
                                                <div class="label">
                                                    <div class="label-text">
                                                        <a class="btn btn-sm priview">
                                                            <i class="fa fa-search"></i> 预览
                                                        </a>
                                                        <a class="btn btn-sm use">
                                                            <i class="fa fa-check"></i> 使用
                                                        </a>
                                                    </div>
                                                    <div class="label-bg"></div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6 mix mix_all" d-name="_Layout2">
                                            <div class="mix-inner">
                                                <div class="img-box">
                                                    <img class="img-responsive" src="/Areas/Adm/Images/_Layout2.png">
                                                </div>
                                                <div class="label">
                                                    <div class="label-text">
                                                        <a class="btn btn-sm priview">
                                                            <i class="fa fa-search"></i> 预览
                                                        </a>
                                                        <a class="btn btn-sm use">
                                                            <i class="fa fa-check"></i> 使用
                                                        </a>
                                                    </div>
                                                    <div class="label-bg"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>
                        <li class="dropdown dropdown-dark">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <span id="userRoleName"></span>
                                <span class="badge blue">切换角色</span>
                            </a>
                            <ul class="dropdown-menu dropdown-menu-default" id="switchRolesBox"></ul>
                        </li>
                        <li class="droddown dropdown-separator">
                            <span class="separator"></span>
                        </li>
                        <li class="dropdown dropdown-user dropdown-dark">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <span class="username username-hide-on-mobile" id="userAccount"></span>
                                <img alt="" class="img-circle" src="~/Areas/Base/Scripts/Metronicv/assets/admin/layout4/img/avatar9.jpg">
                            </a>
                            <ul class="dropdown-menu dropdown-menu-default">
                                <li>
                                    <a href="javascript:;" onclick="goUrl(this, 'Home', 'Index', { area: ''})" target="_blank">
                                        <i class="icon-home"></i> 前往首页
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:loginOut(true)">
                                        <i class="icon-key"></i> 退出登录
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="page-container">
        <div class="page-sidebar-wrapper">
            <div class="page-sidebar navbar-collapse collapse">
                <ul class="page-sidebar-menu " data-keep-expanded="false" data-auto-scroll="true" data-slide-speed="200"></ul>
            </div>
        </div>
        <div class="page-content-wrapper">
            <div class="page-content" id="page-content">
                <div class="page-head portlet">
                    <div class="page-bar">
                        <ul class="page-breadcrumb" id="page_bar"></ul>
                    </div>
                    <div class="page-toolbar">
                        <div class="btn-group btn-theme-panel">
                            <a href="javascript:;" class="btn dropdown-toggle" data-toggle="dropdown">
                                <i class="icon-settings"></i>
                            </a>
                            <div class="dropdown-menu theme-panel pull-right dropdown-custom hold-on-click">
                                <div class="row">
                                    <div class="col-md-4 col-sm-4 col-xs-12">
                                        <h3>皮肤</h3>
                                        <ul class="theme-colors">
                                            <li class="theme-color theme-color-default" data-theme="default">
                                                <span class="theme-color-view"></span>
                                                <span class="theme-color-name">黑色</span>
                                            </li>
                                            <li class="theme-color theme-color-light" data-theme="light">
                                                <span class="theme-color-view"></span>
                                                <span class="theme-color-name">白色</span>
                                            </li>
                                        </ul>
                                    </div>
                                    <div class="col-md-8 col-sm-8 col-xs-12 seperator">
                                        <h3>风格</h3>
                                        <ul class="theme-settings">
                                            <li>
                                                边框
                                                <select class="layout-style-option form-control input-small input-sm">
                                                    <option value="square" data-border="square">方角</option>
                                                    <option value="rounded"  data-border="rounded">圆角</option>
                                                </select>
                                            </li>
                                            <li>
                                                布局
                                                <select class="layout-option form-control input-small input-sm">
                                                    <option value="fluid" data-layoutBox="fluid">全屏</option>
                                                    <option value="boxed"  data-layoutBox="boxed">居中</option>
                                                </select>
                                            </li>
                                            <li>
                                                头部定位
                                                <select class="page-header-option form-control input-small input-sm">
                                                    <option value="fixed" data-headerMode="fixed">固定</option>
                                                    <option value="default" data-headerMode="default">默认</option>
                                                </select>
                                            </li>
                                            <li>
                                                头部下拉菜单
                                                <select class="page-header-top-dropdown-style-option form-control input-small input-sm">
                                                    <option value="light" data-headerMenu="light">白色</option>
                                                    <option value="dark" data-headerMenu="dark">黑色</option>
                                                </select>
                                            </li>
                                            <li>
                                                菜单定位
                                                <select class="sidebar-option form-control input-small input-sm">
                                                    <option value="fixed" data-menuMode="fixed">固定</option>
                                                    <option value="default" data-menuMode="default">默认</option>
                                                </select>
                                            </li>
                                            <li>
                                                菜单样式
                                                <select class="sidebar-menu-option form-control input-small input-sm">
                                                    <option value="accordion" data-menuStyle="accordion">手风琴</option>
                                                    <option value="hover" data-menuStyle="hover">悬浮</option>
                                                </select>
                                            </li>
                                            <li>
                                                底部定位
                                                <select class="page-footer-option form-control input-small input-sm">
                                                    <option value="fixed" data-footerMode="fixed">固定</option>
                                                    <option value="default" data-footerMode="default">默认</option>
                                                </select>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                @RenderBody()
            </div>
        </div>
    </div>
    <div class="page-footer">
        <div class="page-footer-inner"></div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>

